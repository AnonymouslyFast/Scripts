on player step:
	if y-coord of player < 20:
		player does not have permission "godlyplays"
		player's world = world("ul_skymine2")
		set player's health to 0
		
on death:
	if damage was caused by a projectile or attack:
		set death message to "☠ &d%victim% &fwas killed by &c%attacker%"
	else:
		set death message to "☠ &d%victim% &fDied"